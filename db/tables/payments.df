ADD INDEX "id" ON "payments" 
  AREA "Accounts Index"
  UNIQUE
  PRIMARY
  INDEX-FIELD "id" ASCENDING 

ADD INDEX "sender_account" ON "payments" 
  AREA "Accounts Index"
  INDEX-FIELD "sender_account" ASCENDING 
  INDEX-FIELD "payment_date" ASCENDING 

ADD INDEX "beneficiary_account" ON "payments" 
  AREA "Accounts Index"
  INDEX-FIELD "beneficiary_account" ASCENDING 
  INDEX-FIELD "payment_date" ASCENDING 
  
ADD INDEX "payment_date" ON "payments" 
  AREA "Accounts Index"
  INDEX-FIELD "payment_date" ASCENDING 

ADD INDEX "currency" ON "payments" 
  AREA "Accounts Index"
  INDEX-FIELD "currency" ASCENDING 
  INDEX-FIELD "payment_date" ASCENDING 

ADD INDEX "modify_date" ON "payments" 
  AREA "Accounts Index"
  INDEX-FIELD "modify_date" ASCENDING 

ADD INDEX "modify_user" ON "payments" 
  AREA "Accounts Index"
  INDEX-FIELD "modify_user" ASCENDING 

ADD INDEX "state" ON "payments" 
