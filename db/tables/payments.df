ADD TABLE "payments"
  AREA "Accounts Data"
  DESCRIPTION "ABS Clients data tables"
  DUMP-NAME "payments"
  TABLE-TRIGGER "REPLICATION-WRITE" NO-OVERRIDE PROCEDURE "trg_payments_rw.p" CRC "?" 

ADD FIELD "id" OF "payments" AS int64 
  DESCRIPTION "Record Id"
  FORMAT ">>>>>>>>>>>9"
  INITIAL "0"
  POSITION 2
  MAX-WIDTH 4
  ORDER 10

ADD FIELD "version" OF "payments" AS int64 
  DESCRIPTION "Record version for replication"
  FORMAT ">>>>>>>>>>>9"
  INITIAL "0"
  POSITION 3
  MAX-WIDTH 4
  ORDER 20

ADD FIELD "state" OF "payments" AS int64 
  DESCRIPTION "Record state for replication"
  FORMAT ">>>9"
  INITIAL "0"
  POSITION 4
  MAX-WIDTH 4
  ORDER 30

ADD FIELD "modify_date" OF "payments" AS datetime 
  DESCRIPTION "Record modification date"
  FORMAT "99.99.99 hh:mm:ss"
  INITIAL "now"
  POSITION 5
  MAX-WIDTH 4
  ORDER 40

ADD FIELD "modify_user" OF "payments" AS character 
  DESCRIPTION "Record modification user"
  FORMAT "X(8)"
  INITIAL ""
  POSITION 6
  MAX-WIDTH 8
  ORDER 50

ADD FIELD "create_date" OF "payments" AS datetime 
  DESCRIPTION "Record creation date"
  FORMAT "99.99.99 hh:mm:ss"
  INITIAL "now"
  POSITION 7
  MAX-WIDTH 4
  ORDER 60

ADD FIELD "create_user" OF "payments" AS character 
  DESCRIPTION "Record creation user"
  FORMAT "X(8)"
  INITIAL ""
  POSITION 8
  MAX-WIDTH 8
  ORDER 70

  
  
  
  
ADD FIELD "payment_type" OF "payments" AS char 
  DESCRIPTION "Transaction number"
  FORMAT "X(10)"
  INITIAL "0"
  POSITION 9
  MAX-WIDTH 12
  ORDER 80

ADD FIELD "payment_date" OF "payments" AS date 
  DESCRIPTION "Payment registration date"
  FORMAT "99.99.9999"
  INITIAL ?
  POSITION 10
  MAX-WIDTH 4
  ORDER 90

ADD FIELD "amount" OF "payments" AS decimal 
  DESCRIPTION "Amount"
  FORMAT "-z,zzz,zzz,zz9.99"
  INITIAL "0"
  POSITION 11
  MAX-WIDTH 1
  ORDER 100

ADD FIELD "currency" OF "payments" AS character 
  DESCRIPTION "Currency"
  FORMAT "X(3)"
  INITIAL ""
  POSITION 12
  MAX-WIDTH 3
  ORDER 110
  
ADD FIELD "details" OF "payments" AS character 
  DESCRIPTION "Transation details in residence country national language"
  FORMAT "X(80)"
  INITIAL ""
  POSITION 13
  MAX-WIDTH 80
  ORDER 120
  
ADD FIELD "urgency" OF "payments" AS character 
  DESCRIPTION "Urgency: statdart/express"
  FORMAT "X(10)"
  INITIAL ""
  POSITION 14
  MAX-WIDTH 10
  ORDER 130

ADD FIELD "value_date" OF "payments" AS date 
  DESCRIPTION "Value date"
  FORMAT "99.99.9999"
  INITIAL ?
  POSITION 15
  MAX-WIDTH 10
  ORDER 140

  
  
  
